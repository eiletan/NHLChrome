{
    "name": "NHLChrome",
    "description": "Single Game Tracker",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://statsapi.web.nhl.com/"
      ]
  }